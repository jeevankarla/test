    <menu name="RequestItemTabBar" extends="CommonTabBarMenu" extends-resource="component://common/widget/CommonMenus.xml" selected-menuitem-context-field-name="subTabButtonItem3">
        
        <menu-item name="ViewRequestItem" title="Indent Item Overview">
        	<condition disabled-style="disabled">
                <and>
                    <if-has-permission permission="ORDERMGR" action="_CRQDETAIL_UPDATE"/>                                                                    
                    <not><if-empty field="custRequestId"/></not>
                    <not><if-empty field="custRequestItemSeqId"/></not>
                    <if-compare field="custRequest.statusId" operator="not-equals" value="IND_CLOSED"/>
                </and>
             </condition>
            <link target="ViewRequestItem">
                <parameter param-name="custRequestId" from-field="custRequestItem.custRequestId"/>
                <parameter param-name="custRequestItemSeqId" from-field="custRequestItem.custRequestItemSeqId"/>
            </link>
        </menu-item>
        <menu-item name="editRequestItem" title="Edit Indent Item">
            <condition disabled-style="disabled">
                <and>
                    <if-has-permission permission="ORDERMGR" action="_CRQDETAIL_UPDATE"/>                                                                    
                    <not><if-empty field="custRequestId"/></not>
                    <not><if-empty field="custRequestItemSeqId"/></not>
                    <if-compare field="custRequestItem.statusId" operator="not-equals" value="INDITM_CANCELLED"/>
                    <if-compare field="custRequestItem.statusId" operator="not-equals" value="INDITM_COMPLETED"/>
                    <if-compare field="custRequest.statusId" operator="not-equals" value="IND_CLOSED"/>
                </and>
            </condition>
            <link target="EditRequestItem">
                <parameter param-name="custRequestId" from-field="custRequestItem.custRequestId"/>
                <parameter param-name="custRequestItemSeqId" from-field="custRequestItem.custRequestItemSeqId"/>
            </link>
        </menu-item>
    </menu>
