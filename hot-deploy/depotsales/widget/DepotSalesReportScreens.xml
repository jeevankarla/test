<screen name="DepotSalesReports">
        <section>
            <actions>
               <set field="titleProperty" value="Reports"/>	            
               <set field="tabButtonItem" value="DepotSalesReports"/>
               <set field="tabButtonItem2" from-field="parameters.ReportsType"/>  
               <set field="ReportsType" from-field="parameters.ReportsType"/> 
