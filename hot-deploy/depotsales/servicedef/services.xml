<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>DepotSales Services</description>
    <vendor></vendor>
    <version>1.0</version>

   <service name="approveDepotOrder" engine="java" 
        location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="approveDepotOrder">
        <description>Approve Depot Orders</description>
        <attribute name="orderId" type="String" mode="IN" optional="false"/>
        <attribute name="salesChannelEnumId" type="String" mode="INOUT" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
    </service>
       <service name="CreditapproveDepotSalesOrder" engine="java" 
        location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="CreditapproveDepotOrder">
        <description>Approve Depot Orders</description>
        <attribute name="orderId" type="String" mode="IN" optional="false"/>
        <attribute name="salesChannelEnumId" type="String" mode="INOUT" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
    </service>
    <service name="approvePurchaseOrderWithEmail" engine="java" 
        location="in.vasista.vbiz.depotsales.DepotPurchaseServices" invoke="approvePurchaseOrderWithEmail">
        <description>Approve Depot Orders</description>
        <attribute name="orderId" type="String" mode="IN" optional="false"/>
        <attribute name="statusId" type="String" mode="IN" optional="false"/>
        <attribute name="salesChannelEnumId" type="String" mode="INOUT" optional="true"/>
        <attribute name="partyId" type="String" mode="IN" optional="true"/>
    </service>
    <service name="createCustomPaymentFromPreference" engine="java"
           location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="createCustomPaymentFromPreference">
       <description>Creates a payment using the order payment preference</description>
       <attribute name="orderPaymentPreferenceId" type="String" mode="IN" optional="false"/>
       <attribute name="paymentFromId" type="String" mode="IN" optional="true"/>
       <attribute name="paymentRefNum" type="String" mode="IN" optional="true"/>
       <attribute name="comments" type="String" mode="IN" optional="true"/>
       <attribute name="amount" type="String" mode="IN" optional="true"/>
       <attribute name="eventDate" type="Timestamp" mode="IN" optional="true"/>
       <attribute name="paymentId" type="String" mode="OUT" optional="false"/>
    </service>
    <service name="createOrderPayment" engine="java"
           location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="createOrderPayment">
       <description>Creates a payment using the order payment preference</description>
       <attribute name="paymentDate" type="String" mode="IN" optional="true"/>
       <attribute name="orderId" type="String" mode="IN" optional="true"/>
       <attribute name="partyId" type="String" mode="IN" optional="true"/>
       <attribute name="paymentTypeId" type="String" mode="IN" optional="true"/>
       <attribute name="amount" type="String" mode="IN" optional="true"/>
       <attribute name="comments" type="String" mode="IN" optional="true"/>
       <attribute name="paymentRefNum" type="String" mode="IN" optional="true"/>
       <attribute name="issuingAuthority" type="String" mode="IN" optional="true"/>
       <attribute name="inFavourOf" type="String" mode="IN" optional="true"/>
       <attribute name="advPayments" type="List" mode="IN" optional="true"/>
       <attribute name="allStatus" type="List" mode="IN" optional="true"/>
       <attribute name="advPaymentIds" type="List" mode="IN" optional="true"/>
    </service>
    <service name="createDepotSalesReturnHeader" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="createDepotSalesReturnHeader" >
        <description>Create a new Depot Sales ReturnHeader</description>
        <attribute name="returnHeaderTypeId" type="String" mode="IN" optional="false"/>
        <attribute name="detailReturnsList" mode="IN" type="List" optional="false"/>
        <attribute name="fromPartyId" type="String" mode="IN" optional="false"/>
        <attribute name="productStoreId" type="String" mode="IN" optional="false"/>
        <attribute name="effectiveDate" type="Timestamp" mode="IN" optional="true"/>
        <attribute name="needsInventoryReceive" mode="IN" type="String" optional="true"/>
        <attribute name="userLogin" type="GenericValue" mode="IN" optional="true"/>
        <attribute name="returnId" type="String" mode="OUT" optional="true"/>
    </service>
    <service name="getPartySchemeEligibility" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getPartySchemeEligibility" >
        <description>Create a new Depot Sales ReturnHeader</description>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="schemesMap" type="Map" mode="OUT" optional="true"/>
    </service>
    
    <service name="createIssuanceForDepotOrder" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="createIssuanceForDepotOrder">
        <description>Issue Products to Order</description>
        <attribute name="orderId" type="String" mode="IN" optional="false"/>
        <attribute name="inventoryItemId" type="String" mode="IN" optional="true"/>
        <attribute name="shipmentId" type="String" mode="IN" optional="true"/>
        <attribute name="facilityId" type="String" mode="IN" optional="true"/>
        <attribute name="shipmentId" type="String" mode="OUT" optional="true"/>
        <attribute name="issuedDateTime" mode="IN" type="Timestamp"  optional="true"/>
    </service>
    <service name="realizeStatus" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="realizeStatus">
        <description>Issue Products to Order</description>
        <attribute name="paymentPreferenceId" type="List" mode="IN" optional="false"/>
        <attribute name="allStatus" type="List" mode="IN" optional="true"/>
        <attribute name="paymentStatus" type="List" mode="IN" optional="true"/>
    </service>
     <service name="preferenceCancel" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="preferenceCancel">
        <description>Issue Products to Order</description>
        <attribute name="preferenceIds" type="List" mode="IN" optional="false"/>
        <attribute name="allStatus" type="List" mode="IN" optional="true"/>
    </service>
    
    <service name="createInventoryXferRequest" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="createStockXferRequest">
    	<attribute name="transferDate" mode="IN" type="String" optional="true"/>
    	<attribute name="transferGroupId" mode="IN" type="String" optional="true"/>
    	<attribute name="xferQty" mode="IN" type="BigDecimal" optional="false"/>
    	<attribute name="fromFacilityId" mode="IN" type="String" optional="false"/>
    	<attribute name="toFacilityId" mode="IN" type="String" optional="false"/>
    	<attribute name="productId" mode="IN" type="String" optional="false"/>
    	<attribute name="inventoryItemId" mode="IN" type="String" optional="true"/>
    	<attribute name="statusId" mode="IN" type="String" optional="true"/>
    	<attribute name="transferGroupTypeId" mode="IN" type="String" optional="true"/>
    	<attribute name="comments" mode="IN" type="String" optional="true"/>
    	<attribute name="workEffortId" mode="IN" type="String" optional="true"/>
    </service>
    
    
    <service name="getFacilityDetails" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getFacilityDetails">
        <description>Get Child Categories</description>
        <attribute name="fromFacility" type="List" mode="OUT" optional="true"/>
        <attribute name="toFacility" type="List" mode="OUT" optional="true"/>
    </service>
    <service name="populateBranchDepots" engine="java"
                location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="populateBranchDepots" auth="false">
        <attribute name="productStoreId" mode="IN" type="String" optional="true"/>
        <attribute name="ownerPartyId" mode="OUT" type="String" optional="true"/>
        <attribute name="depotsList" mode="OUT" type="List" optional="true"/>
    </service>
    
    
     <service name="sendPurchaseOrderEmailToParty" engine="simple"
            location="component://order/script/org/ofbiz/order/order/OrderServices.xml" invoke="sendPurchaseOrderEmailToParty" auth="false">
        <description>sendPurchaseOrderEmailToParty  </description>
        <attribute name="sendFrom" type="String" mode="IN" optional="true"/>
        <attribute name="sendTo" type="String" mode="IN" optional="false"/>
        <attribute name="sendCc" type="String" mode="IN" optional="true"/>
        <attribute name="userLogin" type="String" mode="IN" optional="true"/>
        <attribute name="subject" type="String" mode="IN" optional="true"/>
        <attribute name="bodyText" type="String" mode="IN" optional="true" allow-html="safe"/>  
        <attribute name="orderId" type="String" mode="IN" optional="true"/> 
        <attribute name="partyId" type="String" mode="IN" optional="true"/> 
    </service>
	<service name="getCustomerBranch" engine="java"
                location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getCustomerBranch" auth="false">
        <attribute name="partyId" mode="IN" type="String" optional="true"/>
        <attribute name="productStoreList" mode="OUT" type="List" optional="true"/>
    </service>
    
    <service name="getChildCategories" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getChildCategories">
        <description>Get Child Categories</description>
        <attribute name="primaryParentCategoryId" type="String" mode="IN" optional="false"/>
        <attribute name="childCategoriesList" type="List" mode="OUT" optional="true"/>
        <attribute name="productCatAssocMap" type="Map" mode="OUT" optional="true"/>
        <attribute name="productCategoryAssocTypeList" type="List" mode="OUT" optional="true"/>
    </service>
    <service name="getAttributeTypes" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getAttributeTypes">
        <description>Get Child Categories</description>
        <attribute name="productCategoryId" type="String" mode="IN" optional="false"/>
        <attribute name="productAttributesMap" type="Map" mode="OUT" optional="true"/>
        <attribute name="productAttributeTypesList" type="List" mode="OUT" optional="true"/>
        <attribute name="productCategoryAttributeTypesList" type="List" mode="OUT" optional="true"/>
    </service>
    <service name="newProductCreation" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="CreateNewProduct">
        <description>Create New Product</description>
        <attribute name="productCategoryId" type="String" mode="IN" optional="false"/>
        <attribute name="childProductCategoryId" type="String" mode="IN" optional="false"/>
        <attribute name="subChildProductCategoryId" type="String" mode="IN" optional="false"/>
        <attribute name="packingTypes" type="String" mode="IN" optional="false"/>
        <attribute name="spinningTypes" type="String" mode="IN" optional="false"/>
        <attribute name="processingTypes" type="String" mode="IN" optional="false"/>
        <attribute name="ply" type="String" mode="IN" optional="false"/>
        <attribute name="count" type="String" mode="IN" optional="false"/>
        <attribute name="uomTypes" type="String" mode="IN" optional="false"/>
    </service>
    <service name="getCategoryMembers" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getCategoryMembers">
        <description>Get Category Members</description>
        <attribute name="productCategoryId" type="String" mode="IN" optional="false"/>
        <attribute name="productCategoryMembers" type="List" mode="OUT" optional="true"/>
    </service>
    <service name="getProductFeatures" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getProductFeatures">
        <description>Get Child Categories</description>
        <attribute name="productCategoryId" type="String" mode="IN" optional="false"/>
        <attribute name="featuresByCategoryMap" type="Map" mode="OUT" optional="true"/>
        <attribute name="productFeatureCategoryList" type="List" mode="OUT" optional="true"/>
        <attribute name="productFeatureCategoryIdsList" type="List" mode="OUT" optional="true"/>
    </service>
        <service name="getPartyAvailableQuotaBalanceHistory" engine="java" location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getPartyAvailableQuotaBalanceHistory" >
        <description>Create a new Depot Sales ReturnHeader</description>
        <attribute name="partyId" type="String" mode="IN" optional="false"/>
        <attribute name="schemesMap" type="Map" mode="OUT" optional="true"/>
    </service>
    <service name="getSchemeTimePeriodId" engine="java" auth="true" 
        location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="getSchemeTimePeriodId">
        <description>Get Scheme time period</description>
        <attribute name="periodTypeId" mode="IN" type="String" optional="false"/>
        <attribute name="fromDate" mode="IN" type="Timestamp" optional="false"/>
        <attribute name="thruDate" mode="IN" type="Timestamp" optional="false"/>
        <attribute name="schemeTimePeriodIdList" mode="OUT" type="List" optional="true"/>
    </service>
    <service name="createPartyQuotaBalanceHistory" engine="java" auth="false" 
        location="in.vasista.vbiz.depotsales.DepotSalesServices" invoke="createPartyQuotaBalanceHistory">
        <description>create Part yQuota Balance History</description>
        <attribute name="schemeId" mode="IN" type="String" optional="false"/>
        <attribute name="partyId" mode="IN" type="String" optional="false"/>
        <attribute name="productCategoryId" mode="IN" type="String" optional="false"/>
        <attribute name="schemeTimePeriodIdList" mode="IN" type="List" optional="false"/>
        <attribute name="quantity" mode="IN" type="BigDecimal" optional="false"/>
        <attribute name="quota" mode="OUT" type="BigDecimal" optional="true"/>
    </service>
</services>

